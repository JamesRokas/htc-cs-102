<Window x:Class="Week_3_Code_Along.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Week_3_Code_Along"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <StackPanel>
            <TextBlock>Dog Name:</TextBlock>
            <TextBox Name= "DogName" Height="24" Margin="0,0,0.333,0"></TextBox>
            <Button Click="Bark_Button_Click">Bork</Button>
            <Button Click="SayName_Button_Click">Heh</Button>
        </StackPanel>
    </Grid>
</Window>
